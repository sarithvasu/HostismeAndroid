<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <android.support.design.widget.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/titel_na"
        android:theme="@style/AppTheme.AppBarOverlay">

        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="?attr/colorPrimary"
            app:popupTheme="@style/AppTheme.PopupOverlay" />

    </android.support.design.widget.AppBarLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_below="@+id/titel_na"
        android:layout_height="wrap_content">
    <TextView
        android:id="@+id/Date"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:text="Date" />

    <TextView
        android:id="@+id/tv_data_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:text="TextView"
        android:layout_alignLeft="@+id/tv_"
        android:layout_alignStart="@+id/tv_" />

    <TextView
        android:id="@+id/rest.name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/Date"
        android:text="Rest. Name"
        android:layout_toLeftOf="@+id/tv_data_time"
        android:layout_toStartOf="@+id/tv_data_time" />

    <TextView
        android:id="@+id/booking_id"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/rest.name"
        android:text="Booking ID" />

    <TextView
        android:id="@+id/desctiption"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/booking_id"
        android:text="Description"
        android:layout_alignRight="@+id/rest.name"
        android:layout_alignEnd="@+id/rest.name"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/table_no"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"

        android:layout_alignParentStart="true"
        android:layout_below="@+id/desctiption"
        android:text="Table No"
        android:layout_toLeftOf="@+id/tv_descrption"
        android:layout_toStartOf="@+id/tv_descrption" />

    <TextView
        android:id="@+id/order_status"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/desctiption"
        android:layout_alignRight="@+id/desctiption"
        android:layout_below="@+id/table_no"
        android:text="Order total" />

    <TextView
        android:id="@+id/tv_"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/booking_id"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:text="TextView"
        android:layout_toRightOf="@+id/booking_id"
        android:layout_toEndOf="@+id/booking_id" />

    <TextView
        android:id="@+id/tv_booking_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/desctiption"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:text="Sy560002019798"
        android:layout_toEndOf="@+id/booking_id"
        android:layout_toRightOf="@+id/booking_id" />

    <TextView
        android:id="@+id/tv_descrption"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/tv_booking_id"
        android:text="TextView"
        android:layout_toRightOf="@+id/booking_id"
        android:layout_toEndOf="@+id/booking_id" />

    <TextView
        android:id="@+id/tv_table_no"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/table_no"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:text="TextView"
        android:layout_alignLeft="@+id/tv_descrption"
        android:layout_alignStart="@+id/tv_descrption" />

    <TextView
        android:id="@+id/tv_order_status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/tv_table_no"
        android:text="TextView"
        android:layout_alignLeft="@+id/tv_table_no"
        android:layout_alignStart="@+id/tv_table_no" />

    <TextView
        android:id="@+id/status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/order_status"
        android:text="Status"
        android:layout_toLeftOf="@+id/tv_order_status"
        android:layout_toStartOf="@+id/tv_order_status" />

    <TextView
        android:id="@+id/status"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tv_order_status"
        android:layout_alignStart="@+id/tv_order_status"
        android:layout_below="@+id/tv_order_status"
        android:text="TextView" />
    </RelativeLayout>
</RelativeLayout>